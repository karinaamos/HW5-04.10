#include <stdio.h>

int main(){
    int x,y,z;
    scanf("%d %d %d", &x, &y, &z);
    if (x<=y && x<=z && y<=z){
        printf("%d %d %d", x ,y,z);
    }
    if (y<=x && y<=z && x<=z){
        printf("%d %d %d", y ,x,z);
    }
    if (x<=y && x<=z && z<=y){
        printf("%d %d %d", x ,z,y);
    }
    if (y<=x && y<=z && z<=x){
        printf("%d %d %d", y ,z,x);
    }
    if (z<=x && z<=y && y<=x){
        printf("%d %d %d", z ,y,x);
    }
    if (z<=x && z<=y && x<=y){
        printf("%d %d %d", z ,x,y);
    }
    return 0;
}
